{
  "name": "dcloud-apis-boilerplate",
  "version": "1.0.0",
  "description": "Utilize essa estrutura para iniciar o desenvolvimento de um novo servi√ßo",
  "author": "Jackson Arceno",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "migrate:new": "ts-node migrate.config.ts new -t ./migrations/template/new-migration.ts -n",
    "migrate:up": "ts-node -r dotenv/config migrate.config.ts up",
    "migrate:status": "ts-node -r dotenv/config migrate.config.ts status",
    "migrate:down": "ts-node -r dotenv/config migrate.config.ts down -l",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "documentation": "npx @compodoc/compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.1.4",
    "cache-manager": "^5.2.1",
    "cache-manager-redis-yet": "^4.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.3",
    "mongoose": "^6.9.0",
    "redis": "4.6.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuidv4": "^6.2.13",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "29.3.1",
    "mongo-migrate-ts": "^1.1.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
